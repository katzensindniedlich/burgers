---
const { to } = Astro.props
---


<html>
    <head>
        <meta charset=utf-8>
        <meta http-equiv=refresh content={`0;url=${to}`}>
        <link rel=canonical href={to}>
        <meta name=robots content=noindex>
        <script define:vars={{ to }}>location.replace(to)</script>
        <title>{to}</title>
        <link rel=prefetch href={to} as=document>
    </head>
</html>